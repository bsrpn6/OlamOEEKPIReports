  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="PeriodID">
          <DataField>PeriodID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="EquipTag">
          <DataField>EquipTag</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="System">
          <DataField>System</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="OEE">
          <DataField>OEE</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="Avail">
          <DataField>Avail</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>KPIs</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT PeriodID, EquipTag, System, ((SUM(Lbs) * YieldAdjust) / (ProductionRate * 1000000.0 / 1440.0)) AS OEE, (SUM(RunMins) /SUM(AvailMins)) AS Avail
	FROM (
		SELECT kd.periodid, eq.EquipTag, eq.System, AVG(kd.lbs) Lbs, MAX(sy.YieldAdjust) YieldAdjust, MAX(sy.ProductionRate) ProductionRate, SUM(kd.RunMins) AS RunMins, SUM(kd.AvailMins) AS AvailMins FROM dbo.vkpiPeriodData kd
			INNER JOIN dbo.kpiEquipment eq ON kd.EquipTag = eq.EquipTag
			INNER JOIN vSystemsWithOrderAndLossRef sy ON sy.System = eq.System
		WHERE kd.TimeStamp &gt;  @StartDate
			AND kd.TimeStamp &lt; @EndDate
			GROUP BY eq.EquipTag, eq.System, kd.PeriodID) a
	GROUP BY PeriodID, EquipTag, System, YieldAdjust, ProductionRate</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
  </DataSets>
