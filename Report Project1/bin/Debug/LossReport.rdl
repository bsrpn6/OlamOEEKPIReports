          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT dt.System, dt.Reason1, dt.Reason2, dt.Comment, DATEADD(n, dt.StartMinuteOffset, pr.PeriodStart) As StartTime FROM(
SELECT System, 
	Reason1,
	Reason2,
	Comment,
	StartMinuteOffset,
	CASE LossCategory 
		WHEN 'A' THEN 'Availability' 
		ELSE 'Performance' END AS LossType,
	StartPeriodID
FROM kpiLossHistory
WHERE System Like  (
	CASE @System 
		WHEN 'All' THEN '%'
		WHEN 'Dice' THEN 'Dice%'
		WHEN 'Paste' THEN 'Paste%' 
		ELSE @System
	END)
) dt
INNER JOIN kpiPeriods pr
ON dt.StartPeriodID = pr.PeriodID
WHERE pr.PeriodStart &gt; @StartDate
AND pr.PeriodEnd &lt; @EndDate</CommandText>
      </Query>
