          <DataField>Comment</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="StartTime">
          <DataField>StartTime</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>KPIs</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT dt.System, dt.Reason1, dt.Reason2, dt.Comment, DATEADD(n, dt.StartMinuteOffset, pr.PeriodStart) As StartTime FROM(
SELECT System, 
	Reason1,
	Reason2,
	Comment,
	StartMinuteOffset,
	CASE LossCategory 
		WHEN 'A' THEN 'Availability' 
		ELSE 'Performance' END AS LossType,
	StartPeriodID
FROM kpiLossHistory
WHERE System Like  (
	CASE @System 
		WHEN 'All' THEN '%'
		WHEN 'Dice' THEN 'Dice%'
		WHEN 'Paste' THEN 'Paste%' 
		ELSE @System
	END)
) dt
INNER JOIN kpiPeriods pr
ON dt.StartPeriodID = pr.PeriodID
WHERE pr.PeriodStart &gt; @StartDate
AND pr.PeriodEnd &lt; @EndDate</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@System">
            <Value>=Parameters!System.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
